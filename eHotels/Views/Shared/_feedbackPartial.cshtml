
@if (TempData["SuccessMessage"] != null)
{
    <div id="successMessage" class="row" style="padding-top: 56px;">
        <div id="alert_space" class="col-md-12 text-center">
            <div id="alert_mes" class="alert alert-success alert-dismissible">
                @TempData["SuccessMessage"]
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
        </div>
    </div>
}
else if (TempData["ErrorMessage"] != null)
{
    <div id="successMessage" class="row" style="padding-top: 56px;">
        <div id="alert_space" class="col-md-12 text-center">
            <div id="alert_mes" class="alert alert-danger alert-dismissible">
                @TempData["ErrorMessage"]
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
        </div>
    </div>
}